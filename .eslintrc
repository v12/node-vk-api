{
    "env": {
        "es6": true,
        "node": true
    },
    "ecmaFeatures": {
        "arrowFunctions": true,
        "binaryLiterals": true,
        "blockBindings": true,
        "classes": true,
        "forOf": true,
        "generators": true,
        "objectLiteralShorthandMethods": true,
        "objectLiteralShorthandProperties": true,
        "octalLiterals": true,
        "templateStrings": true
    },
    "rules": {
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-ex-assign": 2,
        "quote-props": [
            2,
            "as-needed"
        ],
        "no-unreachable": 2,
        "valid-typeof": 2,
        "no-fallthrough": 2,
        "quotes": [
            2,
            "single",
            "avoid-escape"
        ],
        "comma-spacing": 2,
        "semi": 2,
        "space-infix-ops": 2,
        "space-before-function-paren": [
            2,
            "always"
        ],
        "space-before-blocks": [
            2,
            "always"
        ],
        "new-parens": 2,
        "max-len": [
            2,
            120,
            2
        ],
        "no-multiple-empty-lines": [
            2,
            {
                "max": 2
            }
        ],
        "eol-last": 0,
        "no-trailing-spaces": 2,
        "keyword-spacing": 2,
        "prefer-const": 2,
        "strict": [
            2,
            "global"
        ],
        "no-undef": 2
    }
}